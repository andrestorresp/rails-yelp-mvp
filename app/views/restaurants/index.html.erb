<h1>List of Restaurants</h1>

<ul>
  <% @restaurants.each do |restaurant| %>
    <li> <strong><%= restaurant.name %></strong>
    <%= link_to "See details", restaurant_path(restaurant), class: "btn btn-primary"%> </li>
  <% end %>
</ul>

<%= link_to "Add a restaurant", new_restaurant_path(@restaurant) %> |
